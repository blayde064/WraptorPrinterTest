<Window x:Class="WraptorPrinterTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WraptorPrinterTest"
        mc:Ignorable="d"
        Title="Wraptor Print Test" Height="215" Width="301" Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="tbName" Margin="53,33,18,0" TextWrapping="Wrap" Height="23" VerticalAlignment="Top"/>
        <Label Content="Label Name:" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top"/>
        <Label Content="Number of Labels:" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbNum" Margin="53,87,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="120"/>
        <Button x:Name="btnPrint" Content="Print" Margin="0,132,18,15" Click="BtnPrint_Click" HorizontalAlignment="Right" Width="75"/>
        <Button x:Name="btnVerify" Content="Verify" Margin="0,132,98,15" HorizontalAlignment="Right" Width="75" Click="BtnVerify_Click"/>

    </Grid>
</Window>
